  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx run-many --target=serve --all --parallel",
    "build": "nx run-many --target=build --all --parallel --max-parallel=3",
    "test": "nx run-many --target=test --all --parallel --max-parallel=2",
    "lint": "nx run-many --target=lint --all --parallel --max-parallel=3",
    "extract-i18n": "ng extract-i18n --out-file messages.en.xlf --output-path apps/web/src/locale",
    "merge-i18n": "cross-env API_KEY_FILE=.apikey xliffmerge --profile xliffmerge.json",
    "i18n": "npm run extract-i18n && npm run merge-i18n",
    "maildev": "maildev --ip 127.0.0.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.1.2",
    "@angular/cdk": "13.1.2",
