{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/expenses-ng/src",
  "prefix": "jr",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/expenses-ng",
        "index": "apps/expenses-ng/src/index.html",
        "main": "apps/expenses-ng/src/main.ts",
        "polyfills": "apps/expenses-ng/src/polyfills.ts",
        "tsConfig": "apps/expenses-ng/tsconfig.app.json",
        "assets": [
          "apps/expenses-ng/src/favicon.ico",
          "apps/expenses-ng/src/assets"
        ],
        "styles": ["apps/expenses-ng/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/expenses-ng/src/environments/environment.ts",
              "with": "apps/expenses-ng/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "expenses-ng:build:production"
        },
        "development": {
          "browserTarget": "expenses-ng:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/expenses-ng/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "expenses-ng:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/expenses-ng/**/*.ts",
          "apps/expenses-ng/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/expenses-ng"],
      "options": {
        "jestConfig": "apps/expenses-ng/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
