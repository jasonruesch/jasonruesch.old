{
  "name": "jasonruesch",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "./src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/jasonruesch"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "dependsOn": [
        { "projects": "dependencies", "target": "build", "params": "forward" }
      ],
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "jasonruesch:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "jasonruesch:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "jasonruesch:build:production",
          "hmr": false
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/jasonruesch"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "coverage/jasonruesch"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["./src/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["emails"]
}
