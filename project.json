{
  "name": "jasonruesch",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "./src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/jasonruesch"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve-dev": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "jasonruesch:build",
        "host": "0.0.0.0"
      },
      "configurations": {
        "development": {
          "buildTarget": "jasonruesch:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "jasonruesch:build:production",
          "hmr": false
        }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "vercel dev --listen 4200 --yes"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "jasonruesch:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "jasonruesch:build:development"
        },
        "production": {
          "buildTarget": "jasonruesch:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/jasonruesch"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "coverage/jasonruesch"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["./src/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "jasonruesch:build"
      }
    }
  },
  "tags": []
}
