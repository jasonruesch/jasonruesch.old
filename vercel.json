{
  "framework": "vite",
  "outputDirectory": "dist/jasonruesch",
  "rewrites": [
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html",
      "has": [
        {
          "type": "host",
          "value": "(?!localhost).*"
        }
      ]
    }
  ],
  "devCommand": "nx serve --port $PORT"
}
